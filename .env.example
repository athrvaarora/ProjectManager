# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-auth-domain
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-storage-bucket
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-messaging-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id
REACT_APP_FIREBASE_MEASUREMENT_ID=your-measurement-id

# Jira Configuration
REACT_APP_JIRA_BASE_URL=your-jira-base-url
REACT_APP_JIRA_EMAIL=your-jira-email
REACT_APP_JIRA_API_TOKEN=your-jira-api-token

# OpenAI Configuration
REACT_APP_OPENAI_API_KEY=your-openai-api-key

# SendGrid Configuration (for email invitations)
REACT_APP_SENDGRID_API_KEY=your-sendgrid-api-key
REACT_APP_SENDGRID_FROM_EMAIL=noreply@your-domain.com
REACT_APP_SENDGRID_TEMPLATE_ID=your-sendgrid-template-id

# Application Settings
REACT_APP_API_URL=http://localhost:5000/api
REACT_APP_ENV=development

# Authentication
REACT_APP_AUTH_COOKIE_SECURE=false
REACT_APP_AUTH_COOKIE_DOMAIN=localhost

# Feature Flags
REACT_APP_ENABLE_WORKFLOW_GENERATION=true
REACT_APP_ENABLE_PROJECT_DESCRIPTION=true 